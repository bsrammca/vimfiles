snippet fbs "Faker: BS" !
`ruby -rffaker -e 'puts Faker::Company.bs.capitalize + " via " + Faker::Company.catch_phrase.downcase'`
endsnippet

snippet ls "Faker: Lorem sentence" !
`ruby -rffaker -e 'puts Faker::Lorem.sentence(5)'`
endsnippet

snippet lp "Faker: Lorem paragraph" !
`ruby -rffaker -e 'puts Faker::Lorem.paragraph'`
endsnippet
