snippet travis "travis: node_js"
language: node_js
node_js:
- '8'
cache:
  directories:
  - node_modules
endsnippet

snippet travis "travis: ruby"
language: ruby
rvm:
- 2.0.0
endsnippet

snippet travis "travis: elixir"
language: elixir
elixir:
- '1.2.6'
script:
- mix test --exclude pending
cache:
  directories:
  - _deps
  - build
endsnippet
